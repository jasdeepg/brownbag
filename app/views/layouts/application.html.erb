<!DOCTYPE html>
<html>
<head>
  <title>Brownbag</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="header">
		<%= render "layouts/header" %>
	</div>
	<div id="container">
		<%= yield %>
	</div>
	<div id="footer">
		<%= render "layouts/footer" %>
	</div>
</body>
</html>
